@{
}
@{
    if (ViewBag.A == null || ViewBag.B == null || ViewBag.Op == Operator.Unknown)
    {
        <p>Brak wartości a,b lub błedny operator</p>
    }
    else
    {
        <p>Wynik działania dla @ViewBag.A @ViewBag.Op @ViewBag.B = @ViewBag.Result</p>
    }
}
